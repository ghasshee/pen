contract A() {
	method address a(bytes32 x, u8 b, bytes32 c, bytes32 d) {
        // let u256 a = 1 in let u256 b = 900 in  a + b * b + 1000 ; 
        let rec fac n : u256 -> u256 = if n == 0 then 1 else (fac (n-1)) * n in  fac 10;
        // (fn y:u256 -> fn z:u256 ->  y * 100 - y + z ) 3333333333333 44444444; 
		return pre_ecdsarecover(x, b, c, d) then become A();
	}
}
